.view{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgb(235, 235, 235);
}
.view-item-view{
  background-color: rgb(255, 255, 255);
  float: left;
  width: 100%;
  height: 80px;
  border-radius: 15px;
  margin-left: 5px;
  margin-top: 2px;
}
.view-item-view-play{
  float: right;
  margin-right: 12px;
  margin-top: 2px;
}
.view-item-view-ordernumber{
  float: left;
  margin-top: 2px;
  margin-left: 5px;
}
.view-item-view-state{
  float: left;
  margin-left: 18px;
  margin-top: 3px;
  font-size: 8px;
  color: red;
}
.view-item-view-createdate{
  float: left;
  margin-top: 20px;
  margin-left: 5px;
}